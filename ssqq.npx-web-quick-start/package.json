{
  "name": "ssqq-web",
  "version": "1.0.5",
  "description": "一个兼容 OneBot 的非官方网页版 QQ 客户端，Web 端快速启动包。",
  "main": "index.ts",
  "bin": {
    "ssqq-web": "bin/index.js"
  },
  "scripts": {
    "start": "tsc && node bin/index.js -- hostname=0.0.0.0 port=8080",
    "start:local": "tsc && node bin/index.js -- hostname=0.0.0.0 port=8080 skip-update",
    "build": "tsc",
    "test": "tsc && node bin/index.js help"
  },
  "repository": "https://github.com/Stapxs/Stapxs-QQ-Lite-2.0",
  "author": "Stapx Steve [林槐]",
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "log4js": "^6.9.1",
    "request": "^2.88.2",
    "semver-compare": "^1.0.0",
    "typescript": "^5.6.3",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {}
}
